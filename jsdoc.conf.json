{
    "plugins": ["plugins/markdown"],
    "recurseDepth": 10,
    "opts": {
        "recurse": true,
        "destination": "/var/www/html/modbot/docs/",
        "template": "./node_modules/postman-jsdoc-theme"
    },
    "source": {
        "include": ["."],
        "includePattern": ".+\\.js(doc|x)?$",
        "excludePattern": "(node_modules|blank_command)"
    },
    "tags": {
        "dictionaries": ["jsdoc", "closure"]
    },
    "templates": {
        "cleverLinks": true
    }
}